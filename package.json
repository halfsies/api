{
  "name": "api-halfsies-co",
  "version": "0.0.0",
  "description": "tinder for losing money",
  "main": "dist",
  "scripts": {
    "dev": "DEBUG=api-halfsies-co nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "DEBUG=api-halfsies-co node dist",
    "test": "eslint src"
  },
  "engines": {
    "node": "6.4.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "homepage": "https://api.halfsies.co",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halfsies/api"
  },
  "author": {
    "name": "Maxmillion McLaughlin",
    "email": "npm@maxmclau.com"
  },
  "license": "Unlicense",
  "dependencies": {
    "assert": "^1.4.1",
    "body-parser": "^1.13.3",
    "debug": "2.2.0",
    "express": "^4.13.3",
    "express-healthcheck": "0.1.0",
    "express-status-monitor": "^0.0.9",
    "matchmaker": "^0.1.0",
    "mongodb": "^2.2.10",
    "mongoose": "^4.6.1",
    "socket.io": "^1.4.8",
    "twilio": "^2.11.0",
    "yamljs": "0.2.8"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "keywords": [
    "halfsies",
    "tinder",
    "vancouver"
  ]
}
